{
  "settings": {
    "analysis": {
      "analyzer": {
        "repository_analyzer": {
          "type": "stop",
          "stopwords": ["git", "www", "http", "https"]
        }
      }
    }
  },
  "mappings": {
    "dynamic": true,
    "dynamic_templates": [
      {
        "date_as_keyword": {
          "match_mapping_type": "date",
          "mapping": { "type": "keyword" }
        }
      },
      {
        "boolean_as_keyword": {
          "match_mapping_type": "boolean",
          "mapping": { "type": "keyword" }
        }
      },
      {
        "long_as_keyword": {
          "match_mapping_type": "long",
          "mapping": { "type": "keyword" }
        }
      },
      {
        "double_as_keyword": {
          "match_mapping_type": "double",
          "mapping": { "type": "keyword" }
        }
      },
      {
        "binary_as_keyword": {
          "match_mapping_type": "binary",
          "mapping": { "type": "keyword" }
        }
      }
    ],
    "properties": {
      "runId": {
        "type": "keyword"
      },
      "sessionId": {
        "type": "keyword"
      },
      "state": {
        "type": "keyword"
      },
      "startTime": {
        "type": "date_nanos"
      },
      "completeTime": {
        "type": "date_nanos"
      },
      "repository": {
        "type": "text",
        "analyzer": "repository_analyzer"
      },
      "exitStatus": {
        "type": "integer"
      },
      "commandLine":{
        "type": "text",
        "analyzer": "simple"
      },
      "errorReport": {
        "type": "text"
      },
      "success": {
        "type": "boolean"
      },
      "duration": {
        "type": "long"
      }
    }
  }
}