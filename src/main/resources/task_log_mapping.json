      },
      "name": {
        "type": "keyword"
      },
      "process": {
        "type": "keyword"
      },
      "tag": {
        "type": "keyword"
      },
      "container": {
        "type": "keyword"
      },
      "attempt": {
        "type": "integer"
      },
      "state": {
        "type": "keyword"
      },
      "submitTime": {
        "type": "date_nanos"
      },
      "startTime": {
        "type": "date_nanos"
      },
      "completeTime": {
        "type": "date_nanos"
      },
      "exit": {
